{
    "brackets": [
        ["{", "}"]
    ],
    "autoClosingPairs": [
        ["%{", "}%"],
        ["${", "}"]
    ],
    "surroundingPairs": [
        ["%{", "}%"],
        ["${", "}"]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*(?:%\\{|%[^\\{].*?:)",
            "end": "^\\s*(?:\\}%|%\\s*end\\b)"
        }
    }
}